<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://beust.com/testng/testng-1.0.dtd" >
<suite name="Fast Tests">
	<test name="fast">
		<groups>
			<run>
				<exclude name="broken" />
				<exclude name="in-development" />
				<include name="init" />
				<include name="fast" />
			</run>
		</groups>
		<packages>
			<package name="edu.upenn.cis.ppod" />
			<package name="edu.upenn.cis.ppod.*" />
		</packages>
	</test>
</suite>